<?xml version="1.0" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <target name="Console" xsi:type="Console"></target>
  </targets>

  <rules>
    <logger name="*" minlevel="Info" writeTo="Console" />
  </rules>
</nlog>